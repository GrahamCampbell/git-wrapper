{
    "name": "graham-campbell/git-wrapper",
    "description": "A PHP wrapper around the Git command line utility.",
    "keywords": ["git", "git wrapper", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk",
            "homepage": "https://github.com/GrahamCampbell"
        },
        {
            "name": "Chris Pliakas",
            "email": "opensource@chrispliakas.com",
            "homepage": "https://github.com/cpliakas"
        },
        {
            "name": "Tomas Votruba",
            "email": "tomas.vot@gmail.com",
            "homepage": "https://github.com/TomasVotruba"
        }
    ],
    "require": {
        "php": "^7.4.15 || ^8.0.2",
        "symfony/process": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "ondram/ci-detector": "^4.1",
        "phpunit/phpunit": "^9.6.3 || ^10.0.12",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "symfony/filesystem": "^4.4 || ^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\GitWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\GitWrapper\\Tests\\": "tests"
        }
    },
    "suggest": {
        "monolog/monolog": "Enables logging of executed git commands"
    },
    "config": {
        "preferred-install": "dist"
    }
}
